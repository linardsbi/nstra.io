---
import SectionTitle from "@/components/SectionTitle.astro";
---

<section id="contact" class="flex mb-20 w-full pt-44">
  <div class="w-1/3 flex flex-col gap-y-6 relative">
    <div class="sticky top-40 pt-16">
      <SectionTitle> Book a call </SectionTitle>
      <p class="text-lg text-center md:text-xl">
        Level up your business TODAY!
      </p>
    </div>
  </div>

  <div
    class="calendly-inline-widget w-2/3"
    data-url="https://calendly.com/nstra_consulting/30min?primary_color=ffc000"
    style="min-width:320px;height:1000px;"
  >
  </div>
</section>
